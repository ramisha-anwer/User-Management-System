<% for(var i = 0; i < users.length; i++) { %>
    <tr>
        <td><%= i + 1 %></td>
        <td><%= users[i].username %></td>
        <td><%= users[i].email %></td>
        <td><%= users[i].gender %></td>
        <td><%= users[i].userstatus %></td>
        <td>
            <a href="/update-user?id=<%= users[i]._id %>" class="border-shadow update">
                <span class="text-gradient"><i class="fas fa-pencil alt fa-gradient"></i></span>
            </a>
            <a  class="btn border-shadow delete" data-id=<%= users[i]._id %>>
                <span class="text-gradient"><i class="fas fa-times fa-gradient"></i></span>
            </a>
        </td>
    </tr>
    <% } %>